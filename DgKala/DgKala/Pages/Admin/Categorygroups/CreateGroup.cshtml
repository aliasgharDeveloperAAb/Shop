@page "{id?}"
@model DgKala.Pages.Admin.Categorygroups.CreateGroupModel
@{
    ViewData["Title"] = "افزودن گروه";
 
}


<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">افزودن نقش جدید</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="row">
    <form method="post" enctype="multipart/form-data">
        <div class="col-md-8">
            <div class="panel panel-primary">
                <div class="panel-heading">
                    گروه 
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="form-group">
                        <label>عنوان نقش</label>
                        <input type="text" asp-for="CategoryGroups.GroupTitle" class="form-control">
                    </div>
                    <input type="submit" value="ذخیره اطلاعات" class="btn btn-success" />
                </div>
                <!-- /.panel-body -->
            </div>
        </div>


    </form>
</div>
